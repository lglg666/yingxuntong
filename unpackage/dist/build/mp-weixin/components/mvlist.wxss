.mv-box{padding-left:0rpx;margin-top:10rpx}.mv-box .nav{font-size:34rpx;font-weight:600;line-height:110rpx;padding-left:30rpx}.mv-box .nav .more{width:150rpx;height:50rpx;line-height:50rpx;text-align:center;margin-top:30rpx;margin-right:32rpx;font-weight:400;font-size:24rpx;border:2rpx solid #e6e6e6;border-radius:50rpx}.mv-box .nv-item{width:337rpx;float:left;margin-left:30rpx;position:relative}.mv-box .nv-item:nth-child(odd){margin-left:16rpx}.mv-box .nv-item .img{display:block;width:100%;margin-bottom:10rpx;height:220rpx;background:#eee;border-radius:10rpx}.mv-box .nv-item .desc{height:64rpx;line-height:32rpx;text-align:center}.mv-box .nv-item .count{font-size:100rpx;color:#fff;opacity:.7;top:calc(50% - 32rpx);cursor:pointer}